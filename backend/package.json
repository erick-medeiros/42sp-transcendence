{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:repl": "nest start --entryFile repl",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --bail --detectOpenHandles",
    "test:integration": "NODE_ENV=test jest --runInBand --bail --roots ../test/integration/",
    "test:integration:ci": "NODE_ENV=ci jest --runInBand --bail --roots ../test/integration/",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --bail --runInBand --config ./test/jest-e2e.json",
    "test:e2e:ci": "NODE_ENV=ci jest --runInBand --bail --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.418.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.6",
    "@types/express-session": "^1.17.8",
    "@types/passport": "^1.0.13",
    "@types/passport-anonymous": "^1.0.3",
    "@types/qrcode": "^1.5.2",
    "@types/uuid": "^9.0.4",
    "argon2": "^0.31.1",
    "axios": "^1.5.0",
    "bull": "^4.11.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-typeorm": "^2.0.0",
    "cookie": "^0.5.0",
    "express-session": "^1.17.3",
    "install": "^0.13.0",
    "node-gyp": "^9.4.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.6.4",
    "@types/passport-oauth2": "^1.4.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
